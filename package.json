{
  "name": "thom-app",
  "version": "0.0.2",
  "description": "app for demoing tech learnings",
  "private": true,
  "scripts": {
    "dev": "webpack serve --config ./webpack/webpack.dev.cjs",
    "build": "pnpm run clean:dist && webpack --config ./webpack/webpack.prod.cjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deps:up": "docker compose up -d",
    "deps:down": "docker compose down --rmi local",
    "deps:restart": "pnpm run deps:down && pnpm run deps:up",
    "clean:dist": "scripts/clean-dist.sh",
    "clean:terraform": "scripts/clean-terraform.sh",
    "compile": "tsc",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomblweed/thom-app"
  },
  "author": "Thomas Newman",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@tanstack/react-query": "^4.35.7",
    "axios": "^1.5.1",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.16.0",
    "thom-components": "^0.1.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "esbuild-loader": "^4.0.2",
    "eslint": "8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.0.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vitest": "^0.34.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
