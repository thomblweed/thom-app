{
  "name": "thom-app",
  "version": "0.0.2",
  "description": "app for demoing tech learnings",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:app": "webpack serve --config ./webpack/webpack.config.cjs --mode development",
    "dev:css": "tailwind -o ./src/tailwind.css --watch",
    "build:dev": "webpack --config ./webpack/webpack.config.cjs --mode development",
    "test": "jest",
    "deps:up": "docker compose up -d",
    "deps:down": "docker compose down --rmi local",
    "deps:restart": "npm run deps:down && npm run deps:up",
    "clean": "scripts/clean.sh",
    "compile": "tsc",
    "lint": "eslint --ignore-pattern esbuild* --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomblweed/thom-app"
  },
  "author": "Thomas Newman",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "dependencies": {
    "axios": "^0.27.2",
    "history": "^5.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.32.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "thom-components": "^0.1.1",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.3",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
  }
}
